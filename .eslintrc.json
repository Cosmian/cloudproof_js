{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard-with-typescript",
    "plugin:jsdoc/recommended",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": [
      "./tsconfig.json"
    ]
  },
  "plugins": [
    "jsdoc"
  ],
  "rules": {
    "@typescript-eslint/no-empty-interface": 0,
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "all",
        "ignoreRestSiblings": false
      }
    ]
  },
  "ignorePatterns": [
    "tests/wasm_lib"
  ]
}
